{
  "title": "BiGbook",
  "description": "生物信息学与基因组学入门教程",
  "author": "adong <youdng@hotmail.com>",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "root": "./gitbook",
  "structure": {
    "readme": "README.md"
  },
  "plugins": [
    "github",
    "expandable-chapters-small",
    "back-to-top-button",
    "page-toc-button",
    "flexible-alerts",
    "tbfed-pagefooter",
    "code"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/adong77/bigbook.git"
    },
    "page-toc-button":{
      "maxTocDepth": 2,
      "minTocSize": 2
    },
    "tbfed-pagefooter": {
      "copyright":"Copyright &copy LIGENE 2021",
      "modify_label": "Latest Updated：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "code":{
      "copyButtons": true
    }
  }

}
